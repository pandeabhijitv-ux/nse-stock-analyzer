{
  "expo": {
    "name": "NSE Stock Analyzer",
    "slug": "nse-stock-analyzer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "AI-Powered NSE Stock Analysis App with Technical and Fundamental Indicators for Indian Market",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nsestockanalyzer.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.nsestockanalyzer.mobile",
      "permissions": ["INTERNET"],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "YOUR_PROJECT_ID_HERE"
      }
    }
  }
}
